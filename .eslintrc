{
  "plugins": ["prettier"],
  "extends": ["eslint:recommended", "react-app", "plugin:prettier/recommended"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "react/require-default-props": "warn",
    "react/default-props-match-prop-types": "warn",
    "react/prop-types": "warn"
  }
}
